#include <iostream>
#include <string>
using namespace std;

int main() {
    string input;
    cout << "Please enter a string: ";
    cin >> input;

    if (input == string(input.rbegin(), input.rend())) {
        cout << input << " is a palindrome";
    } else {
        cout << input << " is not a palindrome";
    }

    return 0;
}
