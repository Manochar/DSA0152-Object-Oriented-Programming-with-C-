#include <iostream>
int main() {
    int a, b;
    std::cout << "Enter the first number: ";
    std::cin >> a;
    std::cout << "Enter the second number: ";
    std::cin >> b;
    std::string result = (a == b) ? "equal" : "not equal";
    std::cout << "The two numbers are " << result << "." << std::endl;

    return 0;
}
